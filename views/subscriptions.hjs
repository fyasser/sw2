<!DOCTYPE html>
<html>
<head>
  <title>Subscriptions</title>
  <link rel='stylesheet' href='/styles/bootstrap.min.css' />
  <link rel='stylesheet' href='/styles/style.css' />
  <script src="/js/jquery-2.2.0.min.js" type="text/javascript"></script>
  <script src="/js/bootstrap.min.js" type="text/javascript"></script>
</head>
<body>
  <script>
    $(document).ready(function () {
      $.ajax({
        type: "GET",
        url: '/api/v1/zones',
        success: function (data) {
          console.log(data);

          // Parse the JSON data
          var jsonData = JSON.parse(data);

          for (let row of jsonData) {
            $('#tbody').append(`
              <tr>
                <td class="text-center">${row.id}</td>
                <td class="text-center">${row.subtype}</td>
                <td class="text-center">${row.zoneid}</td>
                <td class="text-center">${row.userid}</td>
                <td class="text-center">${row.nooftickets}</td>
               
              </tr>
            `);
          }
        },
        error: function (error) {
          alert(error);
          console.log("error message", error.responseText);
        }
      });
    });
  </script>

  <div>
    <nav class="navbar navbar-inverse" style="padding-left:130px;">
      <ul class="nav navbar-nav">
        <li><a href="/dashboard">Home</a></li>
        <li><a href="/stations_example">Stations</a></li>
        <li class="active"><a href="/users">Users</a></li>  
         <li><a href="/test">Stations</a></li>
        <li><a href="/users/add">Add Users</a></li>
        <li><a href="/subscriptions">Subscriptions</a></li>
        <li><a href="/">Logout</a></li>
      </ul>
    </nav>
  </div>

  <div class="container">
    <table class="table table-striped">
      <thead>
        <tr>
          <th class="text-center">id</th>
          <th class="text-center">zoneid</th>
          <th class="text-center">userid</th>
          <th class="text-center">nooftickets</th>
          <th class="text-center">subtype</th>
        </tr>
      </thead>
      <tbody id="tbody"></tbody>
    </table>
  </div>
</body>
</html>
